.header {
  text-align: center;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.header .wrapper {
  z-index: 0;
  padding: 60rpx 25rpx 100rpx 25rpx;
  backdrop-filter: blur(2px);
  background: rgba(9, 9, 9, 0.6);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.header .left {
  width: 50%;
}
.header .left .userRow {
  display: flex;
  align-items: center;
}
.header .left .userRow .profile {
  text-align: center;
  flex: 33%;
}
.header .left .userRow .profile .image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.header .left .userRow .userInfo {
  flex: 70%;
  text-align: left;
  margin-left: 15rpx;
}
.header .left .userRow .userInfo .h6 {
  color: #cdcdcd;
}
.header .left .userRow .userInfo .h4 {
  color: #fafcfc;
  margin-top: 15rpx;
  font-weight: 400;
}
.header .left .line {
  margin-top: 38rpx;
  margin-bottom: 38rpx;
  width: 95%;
  height: 1rpx;
  background-color: #e4e5e5;
}
.header .right {
  text-align: center;
}
.header .right .h1 {
  color: #fafafa;
}
.header .right .lightGreenButton {
  width: 100%;
  position: relative;
  top: 60rpx;
}
.main {
  position: relative;
  background-color: #f7f8fa;
  border-radius: 23px;
  z-index: 1500;
  margin-top: -23px;
  padding-bottom: 100rpx;
}
.main .infoCell {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.main .orderCard {
  background-color: white;
  padding: 35rpx;
}
.main .imageCell {
  display: block;
}
.main .imageCell .image {
  margin-top: 20rpx;
  border-radius: 8px;
  width: 100%;
  height: 600rpx;
}
.main .cellGroup {
  box-shadow: 0px 5px 12px #ebedf0;
}
