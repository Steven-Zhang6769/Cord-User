page {
  background-color: #f7f8fa;
}
.preLogin {
  text-align: center;
}
.preLogin .info {
  text-align: center;
  position: relative;
  top: 300rpx;
  margin: 0px auto;
}
.preLogin .info image {
  width: 215rpx;
  height: 215rpx;
}
.preLogin .info .h2 {
  margin-top: 40rpx;
}
.preLogin .info .lightGreenButton {
  width: 50%;
  margin-top: 45rpx;
}
.postLogin .header {
  text-align: center;
}
.postLogin .header .background-image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1000;
  backdrop-filter: blur(10px);
  background: rgba(9, 9, 9, 0.5);
}
.postLogin .header .background-image image {
  width: 100%;
  height: 50%;
}
.postLogin .header .wrapper {
  padding: 165rpx 0rpx 15rpx 0rpx;
  z-index: -1000;
  backdrop-filter: blur(10px);
  height: 50%;
  background: rgba(9, 9, 9, 0.5);
}
.postLogin .header .wrapper image {
  margin-top: 20rpx;
  width: 190rpx;
  height: 190rpx;
  border-radius: 50%;
}
.postLogin .header .wrapper .h2 {
  margin-top: 10px;
  color: #fafcfc;
}
.postLogin .header .wrapper .buttons {
  margin: 0px auto;
  width: 76%;
  display: flex;
  justify-content: space-between;
}
.postLogin .header .wrapper button {
  width: 30%;
  padding: 5rpx 0rpx;
  display: inline-block;
  background: #fafcfc;
  border-radius: 8px;
  font-family: PingFangSC;
  font-size: 23rpx;
  font-weight: 450;
  color: #000000;
  margin-top: 33rpx;
  margin-bottom: 40px;
}
.postLogin .body {
  position: relative;
  z-index: 1000;
  background-color: #f7f8fa;
  border-radius: 23px;
  margin-top: -20px;
  padding-bottom: 1px;
  padding-top: 30rpx;
}
.postLogin .body .header {
  margin: 0px auto;
  display: flex;
  align-items: baseline;
  justify-content: space-between;
}
.postLogin .body .line {
  margin-top: 21rpx;
  margin-bottom: 21rpx;
  margin: 0px auto;
  width: 90%;
  height: 1rpx;
  background-color: #e4e5e563;
}
.postLogin .body .orderCard {
  background-color: white;
  padding: 20rpx;
  margin-left: -20rpx;
  margin-top: 10rpx;
  opacity: 0.95;
  transform: scale(0.9);
}
.postLogin .body .orderCard .thumb {
  opacity: 0.8;
}
.postLogin .body .setting {
  margin-bottom: 20px;
}
.postLogin .body .setting .cellGroup {
  background: white;
  padding: 0px 15px 0px 15px;
  border-radius: 8px;
}
.postLogin .body .setting button {
  background-color: #fff;
  padding: 0;
  margin: 0;
  font-size: inherit;
  text-align: left;
}
.postLogin .body .setting button::after {
  border: #fff solid;
}
.postLogin .body .setting .cell {
  background-color: white;
  padding: 10px 0px 10px 0px;
}
